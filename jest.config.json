{
  "moduleFileExtensions": [
    "js",
    "json",
    "vue"
  ],
  "transform": {
    "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
    ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
  },
  "transformIgnorePatterns": [
    "<roodDir>/node_modules/(?!@swish)"
  ],
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/$1",
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "mapCoverage": true
}